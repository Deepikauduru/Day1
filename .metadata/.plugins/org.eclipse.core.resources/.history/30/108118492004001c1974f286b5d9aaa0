package que6;
import java.util.Scanner;
public class Y2K {

	public static void main(String[] args) {
		int birth;
		int current;
		int age;
		Scanner sc=new Scanner(System.in);
		birth=sc.nextInt();
		current=sc.nextInt();
		age=sc.nextInt();
		if(current>birth){
			age=current-birth;
		}
		else if(current<birth){
			age=(current+100)-birth;
		}
		System.out.println(age);
		
	
	}

}
