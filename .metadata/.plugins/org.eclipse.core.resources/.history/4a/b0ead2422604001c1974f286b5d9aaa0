package que8;
import java.util.Scanner;
public class labII {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int L1=sc.nextInt();
		System.out.println("Enter x");
		int L2=sc.nextInt();;
		System.out.println("Enter y");
		int L3=sc.nextInt();
		System.out.println("Enter z");
		System.out.println("Enter the lab allocated fir ACE training");
		String acelab=sc.next();
		if(acelab=="L1"){
		if(L2<L3){
			System.out.println("L2 has the minimal seating capacity");
		}
		else{
			System.out.println("L3 has the minimal seating capacity");
		}
		}
		else if(acelab=="L2"){	
		if(L1<L3){
			System.out.println("L1 has the minimal seating capacity");
		}
		else{
			System.out.println("L3 has the minimal seating capacity");
		}
			
		}
		else if(acelab=="L3"){	
		if(L1<L2){
			
		    System.out.println("L1 has the minimal seating capacity");
			}
		else{
			System.out.println("L3 has the minimal seating capacity");
			}
				
			
		
		}

	}

}
