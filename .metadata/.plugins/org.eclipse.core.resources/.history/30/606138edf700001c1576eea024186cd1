package question10;
import java.util.Scanner;
public class Fencing {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		int l=50;
		int b=20;
	
		Scanner SC=new Scanner(System.in);
		l=SC.nextInt();
		b=SC.nextInt();
		System.out.println("Length and Width:"+l*b);
		

	}

}
