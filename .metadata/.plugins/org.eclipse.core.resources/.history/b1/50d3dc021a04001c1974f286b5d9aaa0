package que4;
import java.util.Scanner;
public class FruitSeller {

	public static void main(String[] args) {
		System.out.println("Enter the price of dozen bannanas");
		float cp;
		float sp;
		
		Scanner sc=new Scanner(System.in);
		cp =sc.nextFloat();
		sp =sc.nextFloat();
		if(cp>sp){
			System.out.println(" %.2f%n:" +(cp-sp));
		}
		if(cp<sp){
			System.out.println("Loss%.2f%n:" +(sp-cp));
		}
		

	}

}
