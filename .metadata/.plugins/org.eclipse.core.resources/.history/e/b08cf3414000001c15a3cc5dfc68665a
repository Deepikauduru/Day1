package question2;
import java.util.Scanner;

public class Swapping {

	private static Object sc;

	public static void main(String[] args) {
		int a=10;
		int b=20;
		int temp;
		Scanner sc = new Scanner(System.in);
		a=sc.nextInt();
		b=sc.nextInt();
		temp=sc.nextInt();
		System.out.println("a:"+a);	
		System.out.println("b:"+b);	

	}

}
